{
	"name": "Teampass",
	"id": "teampass",
	"packaging_format": 1,
	"description": {
		"en": "Passwords Manager",
		"fr": "Gestionnaire de mots de passes."
	},
	"version": "2.1.27.15~ynh2",
	"url": "http://www.teampass.net",
	"license": "AGPL-3.0",
	"maintainer": {
		"name": "",
		"email": ""
	},
	"previous_maintainers": [{
		"name": "Maniack Crudelis",
		"email": "maniackc_dev@crudelis.fr"
	}],
	"requirements": {
		"yunohost": ">= 4.2.4"
	},
	"multi_instance": true,
	"services": [
		"nginx",
		"php7.3-fpm",
		"mysql"
	],
	"arguments": {
		"install" : [
			{
				"name": "domain",
				"type": "domain",
				"example": "domain.org"
			},
			{
				"name": "path",
				"type": "path",
				"example": "/teampass",
				"default": "/teampass"
			},
			{
				"name": "password",
				"type": "password",
				"example": "Choose a password"
			}
		]
	}
}
